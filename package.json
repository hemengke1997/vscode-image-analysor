{
  "publisher": "minko",
  "name": "image-manager",
  "displayName": "Image Manager",
  "version": "0.0.25",
  "private": true,
  "description": "Compress, crop and preview images in vscode",
  "type": "module",
  "icon": "assets/logo.png",
  "keywords": [
    "image",
    "viewer",
    "cropper",
    "compress",
    "tinypng",
    "manager",
    "base64",
    "svg",
    "img",
    "pic",
    "png"
  ],
  "license": "MIT",
  "homepage": "https://github.com/hemengke1997/vscode-image-manager-issue",
  "bugs": {
    "url": "https://github.com/hemengke1997/vscode-image-manager-issue"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hemengke1997/vscode-image-manager-issue"
  },
  "categories": [
    "Visualization",
    "Other"
  ],
  "author": "minko",
  "main": "./dist/extension.cjs",
  "activationEvents": [
    "onStartupFinished"
  ],
  "scripts": {
    "ui:dev": "turbo turbo:ui:dev",
    "vsc:dev": "turbo build:i18n turbo:vsc:dev",
    "turbo:ui:dev": "vite -m development",
    "turbo:vsc:dev": "tsup --watch",
    "build": "run-s build:i18n ui:build vsc:build",
    "ui:build": "REPORT=true turbo turbo:ui:build",
    "vsc:build": "turbo turbo:vsc:build",
    "turbo:ui:build": "vite build -m production",
    "turbo:vsc:build": "rm -rf dist && tsup",
    "test": "run-s test:unit",
    "test:unit": "turbo turbo:test:unit",
    "turbo:test:unit": "vitest --run",
    "pack": "vsce package",
    "prepare": "simple-git-hooks",
    "type-check": "turbo turbo:type-check",
    "turbo:type-check": "tsc --noEmit",
    "lint": "turbo turbo:type-check turbo:lint",
    "turbo:lint": "eslint .",
    "commitlint": "commitlint --edit",
    "up": "taze -I -w",
    "build:i18n": "tsx scripts/build-i18n",
    "changelog": "conventional-changelog -p eslint -i CHANGELOG-CODE.md -s -r 0",
    "bump": "bumpp package.json --commit --tag --no-push --all -x \"npm run changelog\"",
    "vscode:prepublish": "npm run build",
    "publish": "vsce publish"
  },
  "contributes": {
    "commands": [
      {
        "command": "image-manager.open-image-manager",
        "title": "%pkg.title%"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "image-manager.open-image-manager",
          "group": "z_commands"
        }
      ]
    },
    "keybindings": [
      {
        "command": "image-manager.open-image-manager",
        "key": "shift+alt+i",
        "mac": "cmd+alt+i"
      }
    ],
    "configuration": {
      "title": "Image Manager Settings",
      "properties": {
        "image-manager.warningSize": {
          "type": [
            "number",
            "boolean"
          ],
          "default": 500,
          "description": "%pkg.warningSize%"
        },
        "image-manager.imageDefaultWidth": {
          "type": "number",
          "default": 100,
          "description": "%pkg.imageDefaultWidth%"
        },
        "image-manager.scaleStep": {
          "type": "number",
          "default": 0.1,
          "description": "%pkg.scaleStep%"
        },
        "image-manager.exclude": {
          "type": [
            "array"
          ],
          "items": {
            "type": "string"
          },
          "default": [],
          "examples": [
            "**/some-dist/**",
            "some-dist",
            "**/*.ico"
          ],
          "description": "%pkg.exclude%"
        },
        "image-manager.imageType": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "default": [
            "svg",
            "png",
            "jpeg",
            "jpg",
            "ico",
            "gif",
            "webp",
            "bmp",
            "tif",
            "apng",
            "tiff",
            "avif"
          ],
          "description": "%pkg.imageType%"
        },
        "image-manager.compress.method": {
          "type": "string",
          "default": "sharp",
          "enum": [
            "sharp",
            "tinypng"
          ],
          "description": "%pkg.method%"
        },
        "image-manager.compress.tinypngKey": {
          "type": "string",
          "default": "",
          "description": "%pkg.tinypngKey%"
        }
      }
    }
  },
  "dependencies": {
    "tinify": "1.7.1"
  },
  "devDependencies": {
    "@ant-design/colors": "^7.0.2",
    "@commitlint/cli": "^18.6.0",
    "@ctrl/tinycolor": "^4.0.3",
    "@minko-fe/commitlint-config": "^2.0.7",
    "@minko-fe/eslint-config": "^2.1.2",
    "@minko-fe/html-transform": "^0.1.17",
    "@minko-fe/lodash-pro": "^0.2.1",
    "@minko-fe/react-hook": "^0.2.2",
    "@minko-fe/tsconfig": "^2.0.7",
    "@minko-fe/vite-config": "^0.2.0",
    "@types/eslint": "^8.56.2",
    "@types/fs-extra": "^11.0.4",
    "@types/micromatch": "^4.0.6",
    "@types/node": "^18.19.14",
    "@types/pixelmatch": "^5.2.6",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "@types/react-highlight-words": "^0.16.7",
    "@types/vscode": "^1.60.0",
    "@types/vscode-webview": "^1.57.5",
    "@vitejs/plugin-react": "^4.2.1",
    "@vscode/vsce": "^2.23.0",
    "antd": "^5.14.0",
    "bumpp": "^9.3.0",
    "classnames": "^2.5.1",
    "context-state": "^2.1.0",
    "conventional-changelog-cli": "^4.1.0",
    "cropperjs": "^1.6.1",
    "eslint": "^8.56.0",
    "execa": "^8.0.1",
    "fast-glob": "^3.3.2",
    "framer-motion": "^11.0.3",
    "fs-extra": "^11.2.0",
    "fuse.js": "^7.0.0",
    "i18next": "^23.8.2",
    "image-size": "^1.1.1",
    "micromatch": "^4.0.5",
    "mime": "^4.0.1",
    "npm-run-all2": "^6.1.2",
    "pixelmatch": "^5.3.0",
    "postcss": "^8.4.35",
    "react": "^18.2.0",
    "react-contexify": "^6.0.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.12",
    "react-highlight-words": "^0.20.0",
    "react-hotkeys-hook": "^4.5.0",
    "react-i18next": "^14.0.5",
    "react-icons": "^5.0.1",
    "sharp": "0.32.6",
    "simple-git-hooks": "^2.9.0",
    "strict-event-emitter": "^0.5.1",
    "tailwind-antd-preset": "^0.0.4",
    "tailwindcss": "^3.4.1",
    "taze": "^0.13.3",
    "ts-key-enum": "^2.0.12",
    "tsup": "6.7.0",
    "tsx": "^4.7.0",
    "turbo": "^1.12.3",
    "typescript": "^5.3.3",
    "un-detector": "^1.1.0",
    "vite": "^5.0.12",
    "vite-plugin-html": "^3.2.2",
    "vite-plugin-i18n-detector": "^2.2.2",
    "vitest": "^0.34.6"
  },
  "engines": {
    "vscode": "^1.60.0"
  },
  "simple-git-hooks": {
    "commit-msg": "npm run commitlint",
    "pre-commit": "npm run lint"
  }
}
