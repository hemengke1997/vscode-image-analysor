{
  "eslint.experimental.useFlatConfig": true,
  "i18n-ally.localesPaths": ["locales", "src/webview/locales"],
  "i18n-ally.pathMatcher": "{locale}.json5",
  "i18n-ally.enabledParsers": ["json5"],
  "i18n-ally.namespace": false,
  "i18n-ally.sourceLanguage": "zh-CN",
  "i18n-ally.displayLanguage": "zh-CN",
  "i18n-ally.keystyle": "nested",
  "i18n-ally.enabledFrameworks": ["react-i18next", "i18next", "vscode"],
  "search.exclude": {
    "**/__snapshots__/**": true,
    "CHANGELOG-CODE.md": true
  },
  "image-manager.appearance.language": "zh-CN"
}
