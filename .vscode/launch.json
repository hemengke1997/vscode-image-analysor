{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Start Extension",
      "type": "extensionHost",
      "request": "launch",
      "runtimeExecutable": "${execPath}",
      "args": ["--extensionDevelopmentPath=${workspaceFolder}", "--disable-extensions"],
      "outFiles": ["${workspaceFolder}/dist/**/*.cjs"],
      "preLaunchTask": "Watch Extension"
    }
  ]
}
